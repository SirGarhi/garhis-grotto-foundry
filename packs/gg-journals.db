},"src":null,"system":{},"ownership":{"default":-1},"flags":{}}],"ownership":{"default":0,"PCQleWS0iS4nC1hT":3},"flags":{"core":{"sourceId":"JournalEntry.PqvoFv1wP2ExGS6V"},"cf":{"id":"temp_k7t8r8jozye"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677961140556,"modifiedTime":1678641928888,"lastModifiedBy":"PCQleWS0iS4nC1hT"},"folder":null,"sort":0,"_id":"tM0TRqTkjyN3IFeP"}
