n /100}`;\n  const lastTime = actor.getFlag(\"midi-qol\", \"venomfangTime\");\n  if (combatTime !== lastTime) {\n\t await actor.setFlag(\"midi-qol\", \"venomfangTime\", combatTime)\n  }\n}\n// How to check that we've already done one this turn?\nreturn {damageRoll: `${numDice}d6[poison] + ${damageBonus}[poison]`, flavor: \"Venomfang\"};","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"core":{"sourceId":"Item.4N81XXiaeuK8KnaZ"},"cf":{"id":"temp_sqs2lyzab2k"},"exportSource":{"world":"development","system":"dnd5e","coreVersion":"10.291","systemVersion":"2.1.5"},"rest-recovery":{"data":{"recovery":{"enabled":false}}}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677135050619,"modifiedTime":1677809042314,"lastModifiedBy":"PCQleWS0iS4nC1hT"},"folder":null,"sort":0,"_id":"iBpeZNp149HBvo0k"}
