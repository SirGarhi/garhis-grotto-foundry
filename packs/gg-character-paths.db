if( essenceSize == 6 ) {\n\t\tconst essenceItem = actor.items.getName(\"Rare Essence\");\n\t}else if( essenceSize == 10 ) {\n\t\tconst essenceItem = actor.items.getName(\"Very Rare Essence\");\n\t}\n\tconsole.log(essenceItem);\n\tif( essenceItem != undefined && essenceItem.system.quantity != 0 ) {\n\t\tawait essenceItem.update({\"system.quantity\": essenceItem.system.quantity-1});\n\t\tawait item.update({\"system.uses.value\": newValue});\n\t} else {\n\t\tlet errorDialog = new Promise((resolve, reject) => {\n\t\t\tnew Dialog({\n\t\t\t// localize this text\n\t\t\ttitle: \"Unable to find Essence\",\n\t\t\tcontent: `<p>Couldn't find an essence of your chosen size to use</p>`,\n\t\t\t\tbuttons: {\n\t\t\t\t\t  ok: {\n\t\t\t\t\t\t  label: \"Ok\",\n\t\t\t\t\t\t  callback: () => resolve()\n\t\t\t\t\t  },\n\t\t\t\t},\n\t\t\t\tdefault: \"ok\"\n\t\t\t}).render(true);\n\t\t});\n\t\tawait errorDialog;\n\t}\n}","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"core":{"sourceId":"Item.uKXSxbQKvfWJPbaM"},"exportSource":{"world":"5e-staging-grounds","system":"dnd5e","coreVersion":"10.291","systemVersion":"2.1.4"},"cf":{"id":"temp_u2vzk3zbwjh"},"rest-recovery":{"data":{"recovery":{"enabled":false}}}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677100233939,"modifiedTime":1678039626013,"lastModifiedBy":"PCQleWS0iS4nC1hT"},"folder":null,"sort":0,"_id":"eJn63R0a1C0GXhYS"}
